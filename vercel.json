{
  
  "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
  "outputDirectory": "."
      "config": 
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    

  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "bookmyseats/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "bookmyseats/wsgi.py"
    }
  ]

